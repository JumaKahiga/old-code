@model RoleLogin.Models.Department

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <table>
        <tr>
            <td><b>Department Id :</b> </td>
            <td>@Html.EditorFor(model => model.DepartmentId)@Html.ValidationMessageFor(model => model.DepartmentId) </td>
        </tr>
        <tr>
            <td><b>Department Name :</b> </td>
            <td>@Html.EditorFor(model => model.DeptName) @Html.ValidationMessageFor(model => model.DeptName)</td>
        </tr>
    </table>
    <fieldset>
        <legend>Department</legend>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
